{
    "manifest_version": 3,
    "name": "把 google 首頁旋轉",
    "description": "旋轉180度讓你看的不清不楚",
    "version": "1.0.0",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.google.com/*"
        ],
        "js": [
          "execute.js"
        ]
      }
    ]
  }